{
    "server": {
      "port": 8080,
      "apiVersion": "v1",
      "apiPrefix": "/api",
      "cors": {
        "origin": "https://production-domain.com",
        "methods": "GET,HEAD,PUT,PATCH,POST,DELETE",
        "preflightContinue": false,
        "optionsSuccessStatus": 204,
        "credentials": true
      }
    },
    "mongo": {
      "uri": "mongodb://mongo:27017/nawy-app"
    },
    "redis": {
      "host": "redis",
      "port": 6379
    },
    "logger": {
      "level": "error"
    }
  }
  