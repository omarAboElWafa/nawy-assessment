{
    "server": {
      "port": 5000,
      "apiVersion": "v1",
      "apiPrefix": "/api",
      "cors": {
        "origin": "*",
        "methods": "GET,HEAD,PUT,PATCH,POST,DELETE",
        "preflightContinue": false,
        "optionsSuccessStatus": 204
      }
    },
    "mongo": {
      "uri": "mongodb://localhost:27017/nawy-app"
    },
    "redis": {
      "host": "127.0.0.1",
      "port": 6379
    },
    "logger": {
      "level": "info"
    }
  }
  